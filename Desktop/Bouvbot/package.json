{
  "name": "BouvBot",
  "version": "0.1.0",
  "description": "Projet de Bot personnalisé pour modération et musique.",
  "main": "main.js",
  "repository": "https://github.com/Bouvmax/BouvBot",
  "author": "Bouvmax",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.1.1",
    "node.js": "^0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-aqua": "^7.2.3"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off"
    }
  }
}
